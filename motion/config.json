{
  "name": "Motion",
  "url": "https://github.com/dcmartin/hassio-addons",
  "version": "0.5.32",
  "slug": "motion",
  "description": "Motion addon from LINUX package https://motion-project.github.io/",
  "startup": "before",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "audio": true,
  "devices": [
  ],
  "hassio_api": true,
  "homeassistant_api": true,
  "privileged": [
    "SYS_ADMIN"
  ],
  "webui": "http://[HOST]:[PORT:7999]/",
  "timeout": 90,
  "tmpfs": "size=256m,uid=0,rw",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "7999/tcp": 7999,
    "8000/tcp": 8000,
    "8080/tcp": 8080,
    "8081/tcp": 8081,
    "8082/tcp": 8082,
    "8090/tcp": 8090,
    "8091/tcp": 8091,
    "8092/tcp": 8092,
    "8093/tcp": 8093,
    "8094/tcp": 8094,
    "8095/tcp": 8095,
    "8096/tcp": 8096,
    "8097/tcp": 8097,
    "8098/tcp": 8098,
    "8099/tcp": 8099,
    "8100/tcp": 8100,
    "8101/tcp": 8101,
    "8102/tcp": 8102,
    "8103/tcp": 8103,
    "8104/tcp": 8104,
    "8105/tcp": 8105,
    "8106/tcp": 8106,
    "8107/tcp": 8107,
    "8108/tcp": 8108,
    "8109/tcp": 8109,
    "8110/tcp": 8110,
    "8111/tcp": 8111,
    "8112/tcp": 8112,
    "8113/tcp": 8113,
    "8114/tcp": 8114,
    "8115/tcp": 8115,
    "8116/tcp": 8116,
    "8117/tcp": 8117,
    "8118/tcp": 8118,
    "8119/tcp": 8119
  },
  "options": {
    "log_level": "info",
    "group": "motion",
    "post_pictures": "center",
    "cameras": [
      {
        "name": "sample",
        "url": "mjpeg://192.168.1.174/img/video.mjpeg"
      }
    ],
    "mqtt": {
      "host": "core-mosquitto",
      "port": "1883",
      "username": "",
      "password": ""
    },
    "cloudant": {
      "url": "",
      "username": "",
      "password": ""
    }
  },
  "schema": {
    "log_level": "str",
    "log_motion": "int?",
    "log_type": "str?",
    "group": "str",
    "device": "str?",
    "username": "str?",
    "password": "str?",
    "timezone": "str?",
    "post_pictures": "str?",
    "cameras": [
      {
        "name": "str",
        "url": "str",
        "palette": "int?",
        "mac": "str?",
        "type": "str?",
        "fov": "int?",
        "fps": "int?",
        "userpass": "str?",
        "port": "port?",
        "quality": "int?",
        "width": "int?",
        "height": "int?",
        "rotate": "int?",
        "threshold": "int?",
        "event_gap": "int?",
        "brightness": "int?",
        "contrast": "int?",
        "saturation": "int?",
        "lightswitch": "int?"
      }
    ],
    "mqtt": {
      "host": "str?",
      "port": "port?",
      "username": "str?",
      "password": "str?"
    },
    "cloudant": {
      "url": "str?",
      "username": "str?",
      "password": "str?"
    },
    "reload": "str?"
  },
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  }
}
