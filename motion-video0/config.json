  "ports_description": {
    "7999/tcp": "UI and API",
    "8080/tcp": "Motion UI and API",
    "8090/tcp": "Camera stream"
  },
  "privileged": [
    "SYS_ADMIN"
  ],
  "reload": true,
  "runtime": {
    "io.containerd.runc.v2": false,
    "io.containerd.runtime.v1.linux": false,
    "nvidia": true,
    "runc": true
