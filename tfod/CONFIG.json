{
  "name": "tfod",
  "url": "https://github.com/dcmartin/hassio-addons",
  "version": "0.0.1",
  "slug": "tfod",
  "description": "TensorFlow Object Detector",
  "startup": "before",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "hassio_api": true,
  "homeassistant_api": true,
  "privileged": [
    "SYS_ADMIN"
  ],
  "webui": "http://[HOST]:[PORT:8997]/",
  "timeout": 30,
  "tmpfs": "size=32m,uid=0,rw",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "80/tcp": 8997
  },
  "options": {
    "model": "ssdlite_mobilenet_v2_coco_2018_05_09",
    "timezone": "America/Los_Angeles"
},
  "schema": {
    "model": "str?",
    "timezone": "str?"
  }
}
